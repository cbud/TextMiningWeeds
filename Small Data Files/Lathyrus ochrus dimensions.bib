 doi = {10.3390/su11236833},
 journal = {Sustainability},
 keywords = {},
 note = {https://www.mdpi.com/2071-1050/11/23/6833/pdf},
 number = {23},
 pages = {6833},
 title = {Peer-Reviewed Literature on Grain Legume Species in the WoS (1980â€“2018): A Comparative Analysis of Soybean and Pulses},
