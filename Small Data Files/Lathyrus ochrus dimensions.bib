@article{pub.1005667151,
 abstract = {Nel presente lavoro vengono presentati i risultati dell'indagine florsitica effettuata nel territorio della Riserva Naturale Regionale â€œCornate e Fosiniâ€, situata sul confine tra le Province di Siena e Grosseto. Dopo una breve descrizione dei lineamenti geomorfologici, climatici e vegetazionali del territorio viene riportato l'elenco della flora vascolare rilevata in seguito ad erborizzazioni, fonti bibliografiche e segnalazioni; essa risulta costituita da un totale di 591 specie ripartite in 85 famiglie e 326 generi; di queste 568 sono specie autoctone mentre 23 sono introdotte. Lo spettro biologico mostra la netta prevalenza delle emicriptofite (42,6%) sulle terofite (25%), indicando una situazione di transizione tra il bioclima subcontinentale e quello mediterraneo; ciÃ² trova conferma anche nell'analisi corologica che rivela una equa ripartizione tra il contingente tetidico ed quello boreale, con numerosi collegamenti con i settori Alpico-appenninico e Balcanico. Tre sono gli endemismi (Centaurea paniculata subsp. carueliana, Cerastium arvense subsp. arvense var. etruscum e Viola etrusca) mentre numerose sono le specie interessanti dal punto di vista fitogeografico e/o conservazionistico, come Allium carinatum subsp. pulchellum, Carduus chrysacanthus, Deschampsia flexuosa, Fritillaria orientalis, Gagea pusilla, Gagea villosa, Ophrys insectifera, Ranunculus millefoliatus, Salix apennina, Sternbergia colchiciflora, Viola kitaiheliana. I dati raccolti hanno permesso di evidenziare che la Riserva costituisce una preziosa area di rifugio per molte specie caldcole xerotolleranti e, grazie alla molteplicitÃ  di ambienti, permette una buon livello di biodiversitÃ  floristica.},
 author = {Frignani, F. and Angiolini, C. and Selvi, F. and De Dominicis, V.},
 doi = {10.1080/00837792.2004.10670777},
 journal = {Webbia},
 keywords = {},
 number = {2},
 pages = {395-455},
 title = {La Flora vascolare della Riserva Naturale Regionale â€œCornate-Fosiniâ€ (Toscana Meridionale)},
