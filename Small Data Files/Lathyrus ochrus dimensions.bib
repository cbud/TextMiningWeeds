 keywords = {},
 number = {1},
 pages = {75-99},
 title = {The Vascular Flora of the â€œBosco di Maviglianoâ€, a Site of Community Importance (SIC) in Northern Calabria (South-Italy)},
 url = {https://app.dimensions.ai/details/publication/pub.1016154567},
 volume = {64},
 year = {2009}
}

